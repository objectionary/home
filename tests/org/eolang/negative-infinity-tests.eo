    eq.
      negative-infinity
      nan

# Test.
[] > negative-infinity-not-eq-int
  not. > @
    eq.
      negative-infinity
      42

# Test.
[] > negative-infinity-not-eq-float
  not. > @
    eq.
      negative-infinity
      42.5

